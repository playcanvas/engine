{
  "name": "examples-browser",
  "version": "0.0.0",
  "description": "Examples browser for the PlayCanvas Engine",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:example:data && npm run build:directory && npm run build:iframes && cross-env NODE_ENV=production rollup -c",
    "build:example:data": "node --es-module-specifier-resolution=node ./scripts/example-data.mjs",
    "build:directory": "node --es-module-specifier-resolution=node ./scripts/example-directory/index.mjs",
    "build:iframes": "node --es-module-specifier-resolution=node ./scripts/iframe/index.mjs",
    "build:thumbnails": "node --es-module-specifier-resolution=node ./scripts/thumbnails.mjs",
    "build:watch": "npm run build:example:data && npm run build:directory && npm run build:iframes && cross-env concurrently --kill-others \"npm run rollup:watch\"",
    "build:debug": "cross-env ENGINE_PATH=../build/playcanvas.dbg.js concurrently --kill-others \"npm run rollup:watch\"",
    "build:profiler": "cross-env ENGINE_PATH=../build/playcanvas.prf.js concurrently --kill-others \"npm run rollup:watch\"",
    "rollup:watch": "rollup -c -w",
    "serve": "serve dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "@playcanvas/eslint-config",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/ban-ts-comment": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "lines-between-class-members": [
        "error",
        "always",
        {
          "exceptAfterSingleLine": true
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/standalone": "^7.14.7",
    "@monaco-editor/react": "^4.2.1",
    "@playcanvas/eslint-config": "^1.0.7",
    "@playcanvas/observer": "1.3.1",
    "@playcanvas/pcui": "2.1.1",
    "@reach/router": "^1.3.4",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.2",
    "@rollup/plugin-replace": "^3.0.1",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.30.0",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "monaco-editor": "^0.25.2",
    "nodemon": "^2.0.15",
    "prettier": "2.4.1",
    "prop-types": "^15.7.2",
    "puppeteer": "^10.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^2.63.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "serve": "^12.0.0",
    "sharp": "^0.28.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "author": "PlayCanvas <support@playcanvas.com>",
  "license": "MIT"
}
