{
    "plugins": [
        "plugins/markdown",
        "./node_modules/jsdoc-tsimport-plugin/index.js",
        "./node_modules/jsdoc-typeof-plugin"
    ],
    "recurseDepth": 10,
    "source": {
        "include": ["src"]
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "docs",
        "encoding": "utf8",
        "recurse": true,
        "template": "./node_modules/@playcanvas/jsdoc-template"
    }
}
